\end_layout

\begin_layout Itemize
Infrastructure • document language: “airstrip”, “bridges”, “half built village”,
 “military property”, “prison camp”, “roads”, “trenches”, “water tank” •
 description: general government or security forces infrastructure.
 
\end_layout

\begin_layout Itemize
